-- CREACION DE TABLA PROVEDORES

CREATE TABLE "PROVEEDORES" (
"ID_PROVEEDOR" INT(4) NOT NULL AUTO_INCREMENT PRIMARY KEY
"CUIT" VARCHAR(13) NOT NULL 
"NOMBRE" VARCHAR(20)
"APELLIDO" VARCHAR(20) NOT NULL
"DIRECCION" VARCHAR(25) 
"TELEFONO" INT(15)
"EMAIL" VARCHAR(30)
"ID_CONDICION_FISCAL" INT NOT NULL FOREING KEY

-- CREACION TABLA PADRES

CREATE TABLE "PADRES" (
"ID_PADRES" INT(4) NOT NULL AUTO_INCREMENT PRIMARY KEY
"NUMERO_CARABANA" VARCHAR(10) NOT NULL
"CLASE" INT(2) FOREIGN KEY
"CATEGORIA" INT(2) NOT NULL FOREIGN KEY
"ID_PROVEEDOR" INT(4) NOT NULL FOREING KEY
"DESCRIPCION" VARCHAR (50)
"FEHCA_INGRESO" DATE NOT NULL
"COSTO" DECIMAL NOT NULL
"FECHA_EGRESO" DATE
"PRECIO_VENTA" DECIMAL
"MOTIVO_EGRESO" VARCHAR (50)

-- CREACION TABLA MADRES

CREATE TABLE "MADRES" (
"ID_MADRES" INT(6) NOT NULL AUTO_INCREMENT PRIMARY KEY
"NUMERO_CARABANA" VARCHAR(10) NOT NULL
"CLASE" INT(2) FOREIGN KEY
"CATEGORIA" INT(2) NOT NULL FOREIGN KEY
"ID_PROVEEDOR" INT(4) NOT NULL FOREING KEY
"COMENTARIO" VARCHAR (50)
"FEHCA_INGRESO" DATE NOT NULL
"COSTO" DECIMAL NOT NULL
"FECHA_EGRESO" DATE
"PRECIO_VENTA" DECIMAL
"MOTIVO_EGRESO" VARCHAR (50)

-- CREACION TABLE CLIENTES

CREATE TABLE "CLIENTES" (
"ID_CLIENTES" INT(4) NOT NULL AUTO_INCREMENT PRIMARY KEY
"CUIT" VARCHAR(13) NOT NULL 
"NOMBRE" VARCHAR(20)
"APELLIDO" VARCHAR(20) NOT NULL
"DIRECCION" VARCHAR(25) 
"TELEFONO" INT(15)
"EMAIL" VARCHAR(30)
"ID_CONDICION_FISCAL" INT NOT NULL FOREING KEY






